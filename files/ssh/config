Host ssh.dev.azure.com
  ControlMaster no
  ControlPath none
  IdentityFile ~/.ssh/id_rsa

Host vs-ssh.visualstudio.com
  ControlMaster no
  ControlPath none
  IdentityFile ~/.ssh/id_rsa

Host bitbucket.org
  ControlMaster no
  ControlPath none
  IdentityFile ~/.ssh/id_rsa

Host *.bitbucket.org
  ControlMaster no
  ControlPath none
  IdentityFile ~/.ssh/id_rsa

Host *.compute.amazonaws.com
  ControlMaster no
  ControlPath none

Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  UseKeychain yes
  AddKeysToAgent yes

  IdentityFile ~/.ssh/id_rsa
  IdentityFile ~/.ssh/id_ed25519
  IdentityFile ~/key-file/private_key

  Compression yes

  TCPKeepAlive yes
  IdentitiesOnly yes

  ControlMaster auto
  ControlPath ~/.ssh/%r_%h_%p
  ControlPersist 5m

  # Get rid of SSH connection delays
  GSSAPIAuthentication no

  ConnectTimeout 150
  ServerAliveInterval 15
  ServerAliveCountMax 10
  ForwardAgent yes
